{
  "name": "sudoku-solver",
  "version": "3.5.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "rimraf dist",
    "prebuild": "pnpm clean",
    "cp3rdpartylicenses": "copyfiles -f dist/sudoku-solver-angular/3rdpartylicenses.txt dist/sudoku-solver-angular/browser",
    "build": "ng build",
    "postbuild": "pnpm cp3rdpartylicenses",
    "predeployghpages": "pnpm build",
    "deployghpages": "gh-pages -d dist/sudoku-solver-angular/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "eslint . --fix --max-warnings 0",
    "format": "prettier --write \"./**/*.{css,scss,html,js,mjs,json,md,ts}\"",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.0.0",
    "@angular/cdk": "21.0.0",
    "@angular/common": "21.0.0",
    "@angular/compiler": "21.0.0",
    "@angular/core": "21.0.0",
    "@angular/forms": "21.0.0",
    "@angular/material": "21.0.0",
    "@angular/platform-browser": "21.0.0",
    "@angular/platform-browser-dynamic": "21.0.0",
    "@angular/router": "21.0.0",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "fast-sudoku-solver": "1.1.23",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.2100.0",
    "@angular-devkit/core": "21.0.0",
    "@angular-devkit/schematics": "21.0.0",
    "@angular/build": "21.0.0",
    "@angular/cli": "21.0.0",
    "@angular/compiler-cli": "21.0.0",
    "@cypress/schematic": "4.1.4",
    "@eslint/js": "9.39.1",
    "@types/jasmine": "5.1.13",
    "angular-eslint": "20.6.0",
    "copyfiles": "2.4.1",
    "cypress": "15.6.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jasmine-core": "5.12.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0"
  },
  "lint-staged": {
    "*.{html,js,mjs,ts}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ],
    "*.{css,scss,json,md}": "prettier --write"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "cypress",
      "esbuild",
      "lmdb",
      "msgpackr-extract"
    ]
  }
}
