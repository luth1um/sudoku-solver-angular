{
  "name": "sudoku-solver",
  "version": "3.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "rimraf dist",
    "prebuild": "pnpm clean",
    "build": "ng build",
    "predeployghpages": "pnpm build",
    "deployghpages": "gh-pages -d dist/sudoku-solver-angular",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-once": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "eslint . --fix --max-warnings 0",
    "format": "prettier --write \"./**/*.{css,scss,html,js,mjs,json,md,ts}\"",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.0.6",
    "@angular/cdk": "18.0.6",
    "@angular/common": "18.0.6",
    "@angular/compiler": "18.0.6",
    "@angular/core": "18.0.6",
    "@angular/forms": "18.0.6",
    "@angular/material": "18.0.6",
    "@angular/platform-browser": "18.0.6",
    "@angular/platform-browser-dynamic": "18.0.6",
    "@angular/router": "18.0.6",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "fast-sudoku-solver": "1.1.13",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1800.7",
    "@angular-devkit/build-angular": "18.0.7",
    "@angular-devkit/core": "18.0.7",
    "@angular-devkit/schematics": "18.0.7",
    "@angular/cli": "18.0.7",
    "@angular/compiler-cli": "18.0.6",
    "@cypress/schematic": "2.5.2",
    "@types/jasmine": "5.1.4",
    "angular-eslint": "18.1.0",
    "cypress": "13.13.0",
    "eslint": "9.6.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "gh-pages": "6.1.1",
    "husky": "9.0.11",
    "jasmine-core": "5.1.2",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "rimraf": "5.0.8",
    "typescript": "5.4.5",
    "typescript-eslint": "7.15.0"
  },
  "lint-staged": {
    "*.{html,js,mjs,ts}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ],
    "*.{css,scss,json,md}": "prettier --write"
  }
}
