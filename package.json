{
  "name": "sudoku-solver",
  "version": "3.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "rimraf dist",
    "format": "prettier --write \"./**/*.{css,scss,html,js,mjs,json,md,ts}\"",
    "lint": "eslint . --fix --max-warnings 0",
    "build": "ng build",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "e2e": "ng e2e",
    "cp3rdpartylicenses": "copyfiles -f dist/sudoku-solver-angular/3rdpartylicenses.txt dist/sudoku-solver-angular/browser",
    "deployghpages": "gh-pages -d dist/sudoku-solver-angular/browser",
    "watch": "ng build --watch --configuration development",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.1.3",
    "@angular/cdk": "21.1.3",
    "@angular/common": "21.1.3",
    "@angular/compiler": "21.1.3",
    "@angular/core": "21.1.3",
    "@angular/forms": "21.1.3",
    "@angular/material": "21.1.3",
    "@angular/platform-browser": "21.1.3",
    "@angular/platform-browser-dynamic": "21.1.3",
    "@angular/router": "21.1.3",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "fast-sudoku-solver": "3.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.2101.2",
    "@angular-devkit/core": "21.1.2",
    "@angular-devkit/schematics": "21.1.2",
    "@angular/build": "21.1.2",
    "@angular/cli": "21.1.2",
    "@angular/compiler-cli": "21.1.3",
    "@cypress/schematic": "5.0.0",
    "@eslint/js": "9.39.2",
    "@types/jasmine": "5.1.15",
    "angular-eslint": "21.2.0",
    "copyfiles": "2.4.1",
    "cypress": "15.10.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jasmine-core": "5.13.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  },
  "lint-staged": {
    "*.{html,js,mjs,ts}": [
      "prettier --write",
      "eslint --fix --max-warnings 0 --no-warn-ignored"
    ],
    "*.{css,scss,json,md}": "prettier --write"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "cypress",
      "esbuild",
      "lmdb",
      "msgpackr-extract"
    ]
  }
}
